# Frontend Environment Variables
# Copy this file to .env in the frontend directory if needed

# API URL (for production)
# VITE_API_URL=https://your-app.vercel.app/api

# For local development, the proxy in vite.config.js handles API requests
# No need to set VITE_API_URL locally

# Gemini AI API Key (Get from https://makersuite.google.com/app/apikey)
VITE_GEMINI_API_KEY=your_gemini_api_key_here
